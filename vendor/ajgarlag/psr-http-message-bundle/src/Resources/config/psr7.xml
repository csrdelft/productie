<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Ajgarlag\Bundle\PsrHttpMessageBundle\Request\ArgumentValueResolver\Psr7ServerRequestResolver">
            <argument type="service" id="Symfony\Bridge\PsrHttpMessage\HttpMessageFactoryInterface" />
        </service>

        <service id="Ajgarlag\Bundle\PsrHttpMessageBundle\EventListener\PsrResponseListener">
            <argument type="service" id="Symfony\Bridge\PsrHttpMessage\HttpFoundationFactoryInterface" />
        </service>

    </services>
</container>
